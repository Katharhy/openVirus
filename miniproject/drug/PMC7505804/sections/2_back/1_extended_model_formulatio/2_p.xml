     <mo>∑</mo>
     <mrow>
      <mi>l</mi>
      <mo>∈</mo>
      <mi>L</mi>
     </mrow>
    </munder>
    <msub>
     <mi>z</mi>
     <mrow>
      <mi>l</mi>
      <mo>|</mo>
      <mi>T</mi>
      <mo>|</mo>
     </mrow>
    </msub>
   </mrow>
  </math>
 </disp-formula>
