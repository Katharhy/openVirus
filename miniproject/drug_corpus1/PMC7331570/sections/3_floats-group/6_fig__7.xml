      <msub>
       <mi>C</mi>
       <mi>p</mi>
      </msub>
      <mo linebreak="goodbreak" linebreakstyle="after">=</mo>
      <mn>0.1</mn>
      <mo>,</mo>
      <msub>
       <mi>C</mi>
       <mi>s</mi>
      </msub>
      <mo linebreak="goodbreak" linebreakstyle="after">=</mo>
      <mn>0.1</mn>
