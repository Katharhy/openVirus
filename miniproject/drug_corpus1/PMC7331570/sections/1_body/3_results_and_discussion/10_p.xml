    <msub>
     <mi>R</mi>
     <mn>0</mn>
    </msub>
    <mo linebreak="goodbreak" linebreakstyle="after">=</mo>
    <mn>2</mn>
   </mrow>
  </math>
 </inline-formula>), 
 <inline-formula>
  <math id="M192" altimg="si133.svg">
   <mrow>
    <mn>0.4</mn>
   </mrow>
  </math>
 </inline-formula> (
 <inline-formula>
