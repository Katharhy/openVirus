       <mo linebreak="badbreak">+</mo>
       <mi>α</mi>
      </mrow>
      <mo stretchy="true">)</mo>
     </mrow>
     <mo linebreak="badbreak">−</mo>
     <mfrac>
      <mn>1</mn>
      <mn>2</mn>
     </mfrac>
     <msup>
      <msub>
       <mi>ρ</mi>
       <mn>4</mn>
