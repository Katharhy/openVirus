     <mi>I</mi>
     <mo stretchy="true">)</mo>
    </mrow>
    <mo linebreak="badbreak">=</mo>
    <mi>ln</mi>
    <mrow>
     <mo stretchy="true">(</mo>
     <msub>
      <mi>I</mi>
      <mn>0</mn>
     </msub>
     <mo stretchy="true">)</mo>
    </mrow>
    <mo linebreak="badbreak">+</mo>
    <munderover>
     <mo>∫</mo>
     <mn>0</mn>
     <mi>t</mi>
    </munderover>
    <mrow>
     <mo stretchy="true">[</mo>
