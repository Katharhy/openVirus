      <mn>2</mn>
     </msup>
     <mo stretchy="true">]</mo>
    </mrow>
    <mi>d</mi>
    <mi>t</mi>
    <mo linebreak="goodbreak">+</mo>
    <munderover>
     <mo>∫</mo>
     <mn>0</mn>
     <mi>t</mi>
    </munderover>
    <msub>
     <mi>ρ</mi>
     <mn>4</mn>
    </msub>
    <mi>d</mi>
    <msub>
     <mi>W</mi>
     <mn>4</mn>
    </msub>
    <mrow>
     <mo stretchy="true">(</mo>
