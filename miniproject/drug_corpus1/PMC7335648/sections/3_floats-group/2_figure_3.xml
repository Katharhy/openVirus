      <mfrac>
       <mrow>
        <mo>(</mo>
        <mi>C</mi>
        <msub>
         <mrow>
          <mi>c</mi>
          <mi>o</mi>
          <mi>v</mi>
          <mi>i</mi>
          <mi>d</mi>
         </mrow>
         <mrow>
          <mi>i</mi>
