    <mi mathvariant="fraktur">B</mi>
    <mrow>
     <mo>(</mo>
     <mi mathvariant="fraktur">Z</mi>
     <mo>)</mo>
    </mrow>
    <mo linebreak="goodbreak">=</mo>
    <mi mathvariant="bold-script">F</mi>
    <mrow>
     <mo>(</mo>
     <msub>
      <mi mathvariant="fraktur">F</mi>
      <mi mathvariant="fraktur">L</mi>
     </msub>
     <mo>,</mo>
     <mi mathvariant="fraktur">B</mi>
     <mrow>
      <mo>(</mo>
      <msub>
       <mi mathvariant="fraktur">Z</mi>
       <mi mathvariant="fraktur">L</mi>
      </msub>
      <mo>)</mo>
     </mrow>
     <mo>)</mo>
    </mrow>
    <mspace width="0.16em"/>
    <mspace width="0.16em"/>
    <mtext>where</mtext>
    <mspace width="0.16em"/>
    <mspace width="0.16em"/>
    <mrow>
     <mo>(</mo>
     <mi mathvariant="fraktur">L</mi>
     <mo linebreak="badbreak">=</mo>
     <mn>0</mn>
     <mo>,</mo>
     <mn>1</mn>
     <mo>,</mo>
     <mo>…</mo>
     <mo>,</mo>
     <mi mathvariant="fraktur">N</mi>
     <mo>)</mo>
    </mrow>
   </mrow>
  </math>
