     <break/>(genus 
     <italic>Flavivirus</italic>)
    </td>
    <td align="center" valign="top" rowspan="1" colspan="1">
     <italic>Culex</italic> spp.
    </td>
    <td align="center" valign="top" rowspan="1" colspan="1">Fever, headache, seizures, encephalitis.</td>
    <td align="center" valign="top" rowspan="1" colspan="1">No</td>
    <td align="center" valign="top" rowspan="1" colspan="1">Yes [
     <xref rid="B16-molecules-25-02796" ref-type="bibr">16</xref>]
    </td>
